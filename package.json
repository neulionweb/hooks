{
  "name": "@neuweb/hooks",
  "version": "0.1.0",
  "description": "Common Hooks",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "prepack": "npm run lint && npm run test -- -u && npm run build",
    "build": "rimraf dist && tsc && rollup -c",
    "server": "cd ./dev_site && node run.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,json}",
    "format:check": "prettier --list-different src/**/*.{js,jsx,ts,tsx,css,json}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neulionweb/hooks.git"
  },
  "keywords": [
    "lib",
    "template",
    "typescript"
  ],
  "files": [
    "assets",
    "dist",
    "styles",
    "README.md",
    "changelog.md"
  ],
  "author": "NEULION WEB DEVELOPER",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.14.5",
    "@hot-loader/react-dom": "^16.11.0+4.12.16",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-standalone": "^6.26.0",
    "chalk": "^3.0.0",
    "codemirror": "^5.52.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^8.3.0",
